{
    admin off
    persist_config off
    auto_https off
    protocols h1
    log {
        output stdout
        format console
    }
}

:{$PORT} {
    basicauth {
	    {$FILEBROWSER_USRNAME} {$FILEBROWSER_PSSWD_HASH}
    }

    file_server {
        root {$RAILWAY_VOLUME_MOUNT_PATH}
        index nil
        browse
        hide lost+found
    }
}